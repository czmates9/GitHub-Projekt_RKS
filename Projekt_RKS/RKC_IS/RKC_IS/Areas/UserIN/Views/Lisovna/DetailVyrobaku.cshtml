@model IEnumerable<RKC_IS.DataAccess.Model.AutoMapping.VYROBA_ZAZNAM>

@{
    ViewBag.Title = "Detail VP";
    Layout = "~/Areas/UserIN/Views/Shared/_LayoutLisovna.cshtml";
}

<h2>Detail VP</h2>


<div class="btn btn-warning">
    <a href="javascript:void(0);" id="backLink">ZPĚT</a>

</div>

<script type="text/javascript">
    $(document).on('click', '#backLink', function () {
        history.go(-1);
    });
</script>




<div class="form-group">
    <div class="col-md-5">
        <h4 style="background:yellow"> Zakázka: @Html.Raw(ViewBag.ZobrazeniCisloZakazka)</h4>
    </div>
</div>

