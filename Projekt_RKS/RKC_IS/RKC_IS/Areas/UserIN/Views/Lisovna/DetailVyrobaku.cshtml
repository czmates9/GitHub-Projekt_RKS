<table class="table">
    <tr>
        <th>
        </th>
        <th>
            Datum
        </th>
        <th>
            Jméno
        </th>
        <th>
            OK
        </th>
        <th>
            NOK
        </th>
        <th>
            Poznámka NOK
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.C_DATE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.USER.Jmeno)  @Html.DisplayFor(modelItem => item.USER.Prijmeni)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OK)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NOK)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Poznamka)
            </td>
        </tr>
    }

    <tr>
        <td>
            Celkem:
        </td>
        <td>
        </td>
        <td>
        </td>
        <td>
            @Html.Raw(ViewBag.ZobrazeniCelkemOK)
        </td>
        <td>
            @Html.Raw(ViewBag.ZobrazeniCelkemNOK)
        </td>
        <td>
        </td>
    </tr>

</table>

